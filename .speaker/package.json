{
	"dependencies": {
		"request": "2.88.2",
		"md5": "2.3.0",
		"reveal-md": "5.3.2",
		"fflate": "0.7.3"
	},
	"overrides": {
		"reveal.js": ">4.3.0"
	},
	"scripts": {
		"keynote": "reveal-md ../README.md --preprocessor preproc.js -css theme/caps.css",
		"keynote-live-reload": "reveal-md ../README.md --preprocessor preproc.js -css theme/caps.css -w",
		"html": "reveal-md ../README.md --preprocessor preproc.js --static ..",
		"pdf": "reveal-md ../README.md --preprocessor preproc.js --print keynote.pdf"
	}
}